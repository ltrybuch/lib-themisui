{
  "arrow_spacing": {
    "level": "error"
  },

  "braces_spacing": {
    "empty_object_spaces": 0,
    "spaces": 0,
    "level": "error"
  },

  "colon_assignment_spacing": {
    "spacing": {
      "left": 0,
      "right": 1
    },
    "level": "error"
  },

  "cyclomatic_complexity": {
    "level": "warn"
  },

  "eol_last": {
    "level": "error"
  },

  "line_endings": {
    "value": "unix",
    "level": "error"
  },

  "max_line_length": {
    "value": 100,
    "level": "error",
    "limitComments": true
  },

  "no_debugger": {
    "level": "error"
  },

  "no_empty_functions": {
    "level": "error"
  },

  "no_empty_param_list": {
    "level": "error"
  },

  "no_interpolation_in_single_quotes": {
    "level": "error"
  },

  "no_stand_alone_at": {
    "level": "error"
  },

  "no_this": {
    "level": "error"
  },

  "prefer_english_operator": {
    "level": "warn"
  },

  "space_operators": {
    "level": "error"
  },

  "spacing_after_comma": {
    "level": "error"
  },

  "no_focused_jasmine_specs": {
    "module": "coffeelint-no-focused-jasmine-specs",
    "level": "error"
  },

  "prefer_double_quotes": {
    "module": "coffeelint-prefer-double-quotes",
    "level": "warn"
  },

  "prefer_fewer_parens": {
    "module": "coffeelint-prefer-fewer-parens",
    "level": "warn"
  }
}
